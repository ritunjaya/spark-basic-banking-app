}

.card__side--front-3 {
  background: linear-gradient(-45deg, #111011, #1d1d1d);

}

.card__side--back {
  -webkit-transform: rotateY(180deg);
          transform: rotateY(180deg);
}

.card__side--back-1 {
  background: linear-gradient(-45deg, #1a1b1c, #f403d1);
}

.card__side--back-2 {
  background: linear-gradient(-45deg, #080808, #2133f3);
}

.card__side--back-3 {
  background: linear-gradient(-45deg, #9a4eff, #2f302f);
}

.card:hover .card__side--front-1,
.card:hover .card__side--front-2,
.card:hover .card__side--front-3 {
  -webkit-transform: rotateY(-180deg);
          transform: rotateY(-180deg);
}

.card:hover .card__side--back {
  -webkit-transform: rotateY(0);
          transform: rotateY(0);
}

.card__title {
